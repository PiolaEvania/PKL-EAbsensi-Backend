{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/extensions": "off",
    "camelcase": "off",
    "consistent-return": "off",
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "no-param-reassign": "off",
    "eqeqeq": "off",
    "no-unused-vars": "off" 
  }
}